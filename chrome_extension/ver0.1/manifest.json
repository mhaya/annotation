{
  "name": "古典籍画像アノテーションシステムプラグイン",
  "description": "画像に対するアノテーションを可能にするプラグインです",
  "version": "0.1",
  "permissions": [
    "activeTab",
    "https://*/*",
    "http://*/*",
      "storage",
      "cookies"
  ],
    "icons": {
	"16": "icon16.png",
	"48": "icon48.png",
	"128": "icon128.png"
   },
  "background": {
      "scripts": ["jquery-1.12.0.min.js","background.js"],
    "persistent": false
  },
  "browser_action": {
      "default_title": "anno",
      "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "manifest_version": 2
}
