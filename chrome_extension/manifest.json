{
  "name": "anno",
  "description": "anno",
  "version": "0.0",
  "permissions": [
    "activeTab",
    "https://*/*",
    "http://*/*",
    "storage"
  ],
  "content_scripts":[{
      "matches":[
	  "http://*/*",
	  "https://*/*"
      ],
      "css":["annotorious.css"],
      "js":["annotorious.min.js","content.js"]
  }],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "anno"
  },
  "manifest_version": 2
}
